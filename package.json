{
  "private": true,
  "name": "Snippets",
  "version": "2.3.0",
  "description": "Create and edit JavaScript code snippets, which are synced to all your computers",
  "scripts": {
    "build": "npm run webpack:config:build && npm run build:prod",
    "webpack:config:build": "tsc --pretty --project ./tsconfig.webpack.json",
    "webpack:config:watch": "tsc --pretty --watch --project ./tsconfig.webpack.json",
    "webpack:config:clear": "rm ./tsconfig.webpack.json",
    "build:prod": "webpack --mode=production --progress",
    "build:dev": "webpack --progress",
    "start": "webpack-dev-server --progress --env.devServer",
    "format": "prettier --write \"./src/**/*.js\""
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/chrome": "0.0.122",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/webpack-cleanup-plugin": "^0.5.2",
    "ajv": "^6.12.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "prettier": "^2.0.5",
    "redux-logger": "^3.0.6",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "stylus-native-loader": "^1.3.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@octokit/rest": "^18.0.3",
    "brace": "^0.11.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-ace": "^9.1.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.75",
    "webext-redux": "^2.1.7"
  }
}
